{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "dist-tests",
    "module": "commonjs",
    "target": "ES2021",
    "moduleResolution": "node",
    "noEmit": false,
    "declaration": false,
    "sourceMap": false,
    "noImplicitAny": false
  },
  "include": [
    "src/lib/**/*.ts",
    "src/app/api/tiktok/**/*.ts",
    "tests/unit/**/*.ts"
  ]
}
